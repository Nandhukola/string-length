import java.util.*;
public class Stringlength
{
 public static void  main(String args[])
  {
   int sum=0;
   Scanner sc=new Scanner(System.in);
   System.out.print("enter string:");
   String str=sc.nextLine();
   String ch[]=str.split(" ");
   for(int i=0;i<ch.length;i++)
   {   
       String s=ch[i];
       sum+=s.length();
    }
  System.out.print(sum);
  }
}
